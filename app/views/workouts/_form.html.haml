= simple_form_for(@workout, html: {class: 'form-horizontal' }) do |f|
  = f.input :date, label: "Date"
  = f.input :workout, label: "Target Muscle", input_html: {class: "form-control"}
  = f.input :mood, label: "Energy Level", input_html: {class: "form-control"}
  = f.input :length, label: "Duration of Workout", input_html: {class: "form-control"}
  %br/
  = f.button :submit
